config_version=5

[Scenematic_FPS]

config/ninetailsrabbit.interaction_kit_3d/update_notification_enabled=true
config/ninetailsrabbit.interaction_kit_3d/interactables_collision_layer=6
config/ninetailsrabbit.interaction_kit_3d/grabbables_collision_layer=7

[application]

config/name="Scenematic_FPS"
config/version="170125-prealpha"
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

ProjectGlobals="*res://scenes/project_globals.gd"
CheckPoint="*res://scenes/checkpoint.gd"
GlobalInteraction3D="*res://addons/ninetailsrabbit.interaction_kit_3d/src/autoload/global_interaction.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"

[display]

window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"

[editor]

movie_writer/disable_vsync=true

[editor_plugins]

enabled=PackedStringArray("res://addons/debug_menu/plugin.cfg", "res://addons/ninetailsrabbit.interaction_kit_3d/plugin.cfg", "res://addons/terrain_3d/plugin.cfg", "res://addons/zylann.hterrain/plugin.cfg")

